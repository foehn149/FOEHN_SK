<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/donationprint"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="inc.thenewpirates.foehn.PrintDonationActivity">

    <WebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/printcommand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal|bottom"
        android:drawableLeft="@drawable/printer"
        android:drawablePadding="6dp"
        android:enabled="true"
        android:onClick="createWebPrintJobDonation"
        android:text="@string/print_string" />

</FrameLayout>
